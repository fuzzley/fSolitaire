{
  "name": "fsolitaire",
  "version": "1.0.0",
  "description": "Solitaire game.",
  "private": true,
  "scripts": {
    "test": "karma start",
    "build": "webpack",
    "tslint": "tslint -p . -c tslint.json '**/*.ts' --exclude '**/*.d.ts'",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "webpack-dev-server": "webpack-dev-server --env.mode development --open",
    "dev": "npm run webpack-dev-server",
    "start": "npm run dev",
    "build:dev": "npm run build -- --env.mode development",
    "build:prod": "npm run build -- --env.mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fuzzley/fSolitaire.git"
  },
