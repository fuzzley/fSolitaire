  },
  "homepage": "https://github.com/fuzzley/fSolitaire#readme",
  "devDependencies": {
    "@types/jasmine": "^2.8.16",
    "concurrently": "^4.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jasmine": "^3.3.1",
    "jasmine-core": "^3.3.0",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-jasmine": "^1.1.2",
    "karma-typescript": "^3.0.13",
