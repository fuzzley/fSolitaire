{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single"],
        "eofline": false,
        "interface-name": [true, "never-prefix"],
        "no-unused-expression": [true, "allow-new"],
        "no-console": [false],
        "max-line-length": [true, {"limit": 100, "ignore-pattern": "^import |^export {(.*?)}"}]
    },
    "rulesDirectory": []
}